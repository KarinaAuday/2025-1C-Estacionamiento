@model List<Persona>

<p> Las Personas del Repositorio son</p>

<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th>
                Nombre
            </th>
            <th>
                Apellido
            </th>
            <th>
                DNI
            </th>
            <th>
                Email
            </th>
        </tr>
    </thead>

    <tbody>

        @foreach (Persona persona in Model)
        {
            <tr>
                <td>
                    @persona.Nombre
                </td>
                <td>
                    @persona.Apellido
                </td>
                <td>
                    @persona.Dni
                </td>
                <td>
                    @persona.Email
                </td>
                
            </tr>
        }
    </tbody>



</table>
